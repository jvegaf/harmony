{
  "name": "harmony",
  "version": "0.10.1",
  "description": "Music manager for old-school DJs",
  "main": "./out/main/index.js",
  "author": "Jose Vega",
  "license": "MIT",
  "homepage": "https://github.com/jvegaf/Harmony#readme",
  "repository": "https://github.com/jvegaf/Harmony",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:macsilicon": "electron-vite build && electron-builder --mac --arm64 --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "release": "electron-builder"
  },
  "dependencies": {
    "@alexjorgef/bandcamp-scraper": "^1.4.2-1",
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^7.8.0",
    "@mantine/dates": "^7.8.0",
    "@mantine/dropzone": "^7.8.0",
    "@mantine/form": "^7.8.0",
    "@mantine/hooks": "^7.8.0",
    "@mantine/modals": "^7.8.0",
    "@mantine/notifications": "^7.8.0",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-slider": "^1.2.2",
    "@tabler/icons-react": "^2.47.0",
    "ag-grid-community": "^32.0.2",
    "ag-grid-react": "^32.0.2",
    "async-g-i-s": "^1.5.2",
    "axios": "^1.7.9",
    "cherio": "^1.0.0-rc.2",
    "dayjs": "^1.11.10",
    "electron-store": "8.2.0",
    "googlethis": "^1.8.0",
    "lodash": "^4.17.21",
    "music-metadata": "^7.14.0",
    "node-id3": "^0.2.6",
    "react-icons": "^5.4.0",
    "react-keybinding-component": "^2.2.0",
    "recursive-readdir": "^2.2.3",
    "soundcloud.ts": "^0.6.2",
    "sqlite3": "5.1.6",
    "styled-components": "^6.1.11",
    "typeorm": "^0.3.20",
    "wavesurfer.js": "^7.9.0",
    "youtube-music-api": "^1.0.6",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@types/lodash": "^4.17.12",
    "@types/node": "^18.19.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/recursive-readdir": "^2.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "electron": "^33.3.0",
    "electron-builder": "^24.9.1",
    "electron-builder-squirrel-windows": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-log": "^5.2.4",
    "electron-vite": "^2.1.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.33.2",
    "globby": "^14.0.2",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.14.4",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.1",
    "queue": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
